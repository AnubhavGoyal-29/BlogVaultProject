<% start_date = params["history"]["start_date"].present? ? params["history"]["start_date"] : V2::Test.first.created_at %>
<% end_date = params["history"]["end_date"].present? ? params["history"]["end_date"] : V2::Test.last.updated_at %>
<%if params["history"]["Plugin"] %>
  <%= line_chart V2::Plugin.where(:plugin_slug => params["history"]["Plugin"]).first.history(start_date, end_date), 
    :xtitle => "Test Number", :ytitle => "Number of Websites using" %>
<%elsif params["history"]["Theme"]%>
  <%= line_chart V2::Theme.where(:theme_name => params["history"]["Theme"]).first.history(start_date, end_date) %>
<%else %>
  <%= line_chart V2::Test::history_of_tests(start_date, end_date) %>
<%end%>

