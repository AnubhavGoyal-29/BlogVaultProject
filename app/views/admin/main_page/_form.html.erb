<%= form_for :compare_test, builder: ActiveAdmin::FormBuilder,
  :url => admin_main_page_compare_test_path, :method => :post  do |f| %>
  <%= f.input :type, :as => :select, :collection => {:one => 1, :two => 2}, include_blank: false %>
  <%= %>
  <%= submit_tag "Compare Now"%>
<%end%>
<% if params["compare_test"] and params["compare_test"]["test1"] != "" and params["compare_test"]["test2"] != "" %>
  <% test1 = params["compare_test"]["test1"].to_i %>
  <% test2 = params["compare_test"]["test2"].to_i %>
  <% if test1 > test2 %>
    <% temp = test1 %>
    <% test1 = test2 %>
    <% test2 = temp %>
  <% end %>
<% end %>
