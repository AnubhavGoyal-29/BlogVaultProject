<%= semantic_form_for :start_test, builder: ActiveAdmin::FormBuilder,
    :url => admin_dashboard_start_test_path, :method => :post  do |f| %>
  <%= f.inputs "Enter CSV file to proceed" do %>
    <%= f.input :file, :as => :file, input_html: {accept: ".csv"}%>
  <%end%>
  <%=submit_tag "Start Process"%>
<%end%>
